<% if(typeof errors != 'undefined') { %>
    <% errors.forEach((error)=>{ %>
        <p id="messages"><%= error.msg %></p>     
    <% })%>
<% } %>

<% if(success_msg != ''){ %>
    <p id="success_msg"><%= success_msg %></p>
<% } %>

<% if(error_msg != ''){ %>
    <p id="messages"><%= error_msg %></p>
<% } %>

<% if(error != ''){ %>
    <p id="messages"><%= error %></p>
<% } %>
